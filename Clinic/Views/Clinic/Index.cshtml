﻿@model ClinicBO.ClinicModel
@{
    ViewBag.Title = "Clinic List";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<link href="~/Content/themes/base/minified/CustomStyleSheet.css" rel="stylesheet" />

<div class="app-loader"><i class="icofont-spinner-alt-4 rotate"></i></div>

<div class="main-content-wrap">
    <header class="page-header">
        <h3 class="page-title">Clinic</h3>
        <div class="text-right">
            <button type="button" class="btn btn-primary text-right" onclick="RegisterNewClinic();" style="margin: 8px 0px 0px 10px;">New</button>
        </div>
    </header>

    <div class="page-content">
        <div class="card mb-0">
            <div class="card-body">
                <div class="table-responsive">
                    <table id="clinicListGrid"></table>
                </div>
            </div>
        </div>
    </div>
</div>
